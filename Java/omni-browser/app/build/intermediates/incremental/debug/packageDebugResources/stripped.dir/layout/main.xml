<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear24"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="horizontal"
		android:weightSum="2">
		<LinearLayout
			android:id="@+id/linear25"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:layout_weight="1">
			<LinearLayout
				android:id="@+id/linear26"
				android:layout_width="150dp"
				android:layout_height="wrap_content"
				android:gravity="bottom"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="2dp"
					android:background="@android:color/transparent"
					android:text="Enter Password"
					android:textSize="10sp"
					android:textStyle="bold"
					android:textColor="#607D8B"/>
			</LinearLayout>
			<EditText
				android:id="@+id/edittext3"
				android:layout_width="150dp"
				android:layout_height="wrap_content"
				android:layout_margin="5dp"
				android:padding="8dp"
				android:background="#F5F5F5"
				android:gravity="center_horizontal|center_vertical"
				android:textSize="12sp"
				android:textColor="#000000"
				android:hint="P A S S W O R D"
				android:textColorHint="#607D8B"
				android:singleLine="true"/>
			<Button
				android:id="@+id/button11"
				android:layout_width="80dp"
				android:layout_height="35dp"
				android:layout_marginTop="10dp"
				android:padding="8dp"
				android:background="#ECEFF1"
				android:text="CONFIRM"
				android:textSize="12sp"
				android:textColor="#000000"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical"
			android:weightSum="60"
			android:layout_weight="1">
			<LinearLayout
				android:id="@+id/linear2"
				android:layout_width="match_parent"
				android:layout_height="37dp"
				android:paddingLeft="5dp"
				android:paddingRight="5dp"
				android:background="#424242"
				android:gravity="bottom"
				android:orientation="horizontal"
				android:weightSum="5"
				android:layout_weight="1">
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="match_parent"
					android:layout_height="37dp"
					android:layout_marginLeft="10dp"
					android:layout_marginRight="10dp"
					android:background="#FFFFFF"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal"
					android:weightSum="100"
					android:layout_weight="5">
					<LinearLayout
						android:id="@+id/linear17"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:layout_marginLeft="20dp"
						android:layout_marginRight="5dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="5">
						<ImageView
							android:id="@+id/imageview2"
							android:layout_width="15dp"
							android:layout_height="15dp"
							android:src="@drawable/ic_lock_outline_black"
							android:scaleType="centerInside"
							android:layout_weight="5"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear21"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="5">
						<ImageView
							android:id="@+id/imageview1"
							android:layout_width="25dp"
							android:layout_height="15dp"
							android:src="@drawable/google"
							android:scaleType="fitCenter"/>
					</LinearLayout>
					<EditText
						android:id="@+id/edittext1"
						android:layout_width="70dp"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:background="#FFFFFF"
						android:gravity="center_horizontal|center_vertical"
						android:textSize="12sp"
						android:textColor="#424242"
						android:hint="Search Or Enter Address"
						android:textColorHint="#607D8B"
						android:singleLine="true"
						android:lines="1"
						android:imeOptions="actionGo"
						android:layout_gravity="center_horizontal|center_vertical"
						android:layout_weight="78"/>
					<LinearLayout
						android:id="@+id/linear6"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="5">
						<Button
							android:id="@+id/button6"
							android:layout_width="20dp"
							android:layout_height="20dp"
							android:background="@drawable/ic_refresh_black"
							android:textSize="12sp"
							android:textColor="#000000"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear9"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="5dp"
						android:layout_marginRight="20dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="5">
						<Button
							android:id="@+id/button8"
							android:layout_width="20dp"
							android:layout_height="20dp"
							android:padding="8dp"
							android:background="@drawable/ic_arrow_forward_black"
							android:textSize="12sp"
							android:textColor="#000000"/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<ProgressBar
				android:id="@+id/progressbar1"
				android:layout_width="match_parent"
				android:layout_height="5dp"
				android:background="#FFFFFF"
				android:indeterminate="true"
				style="?android:progressBarStyleHorizontal"/>
			<LinearLayout
				android:id="@+id/linear10"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:background="#FFFFFF"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical"
				android:weightSum="100"
				android:layout_weight="58">
				<LinearLayout
					android:id="@+id/linear32"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:padding="8dp"
					android:gravity="center_horizontal|bottom"
					android:orientation="vertical"
					android:layout_weight="55">
					<ImageView
						android:id="@+id/imageview3"
						android:layout_width="220dp"
						android:layout_height="wrap_content"
						android:layout_marginLeft="15dp"
						android:layout_marginTop="15dp"
						android:layout_marginRight="15dp"
						android:layout_marginBottom="20dp"
						android:src="@drawable/logo"
						android:scaleType="centerInside"/>
					<LinearLayout
						android:id="@+id/linear11"
						android:layout_width="match_parent"
						android:layout_height="45dp"
						android:layout_marginLeft="20dp"
						android:layout_marginRight="20dp"
						android:padding="5dp"
						android:background="#424242"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal"
						android:weightSum="50">
						<LinearLayout
							android:id="@+id/linear22"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:layout_weight="1">
							<Spinner
								android:id="@+id/spinner2"
								android:layout_width="15dp"
								android:layout_height="15dp"
								android:padding="8dp"
								android:background="@drawable/google_icn"
								android:spinnerMode="dropdown"/>
						</LinearLayout>
						<EditText
							android:id="@+id/edittext2"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:background="#424242"
							android:gravity="center_horizontal|center_vertical"
							android:textSize="12sp"
							android:textColor="#FFFFFF"
							android:hint="S E A R C H"
							android:textColorHint="#BDBDBD"
							android:singleLine="true"
							android:imeOptions="actionGo"
							android:layout_weight="48"/>
						<LinearLayout
							android:id="@+id/linear23"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:layout_weight="1">
							<Button
								android:id="@+id/button9"
								android:layout_width="20dp"
								android:layout_height="20dp"
								android:padding="8dp"
								android:background="@drawable/ic_search_white"
								android:textSize="12sp"
								android:textColor="#000000"/>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear33"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:paddingLeft="20dp"
					android:paddingTop="8dp"
					android:paddingRight="20dp"
					android:paddingBottom="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal"
					android:weightSum="3"
					android:layout_weight="45">
					<LinearLayout
						android:id="@+id/linear37"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<Button
							android:id="@+id/button13"
							android:layout_width="40dp"
							android:layout_height="45dp"
							android:padding="8dp"
							android:background="@drawable/reddit_4"
							android:textSize="12sp"
							android:textColor="#000000"
							android:alpha="0.7"/>
						<LinearLayout
							android:id="@+id/linear38"
							android:layout_width="match_parent"
							android:layout_height="30dp"
							android:padding="8dp"
							android:orientation="horizontal"/>
						<Button
							android:id="@+id/button16"
							android:layout_width="40dp"
							android:layout_height="40dp"
							android:padding="8dp"
							android:background="@drawable/reddit_1"
							android:textSize="12sp"
							android:textColor="#000000"
							android:alpha="0.7"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear36"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<Button
							android:id="@+id/button14"
							android:layout_width="40dp"
							android:layout_height="40dp"
							android:padding="8dp"
							android:background="@drawable/reddit_3"
							android:textSize="12sp"
							android:textColor="#000000"
							android:alpha="0.7"/>
						<LinearLayout
							android:id="@+id/linear39"
							android:layout_width="match_parent"
							android:layout_height="30dp"
							android:padding="8dp"
							android:orientation="horizontal"/>
						<Button
							android:id="@+id/button17"
							android:layout_width="40dp"
							android:layout_height="40dp"
							android:padding="8dp"
							android:background="@drawable/linkedin"
							android:textSize="12sp"
							android:textColor="#000000"
							android:alpha="0.7"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear34"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<Button
							android:id="@+id/button15"
							android:layout_width="40dp"
							android:layout_height="40dp"
							android:padding="8dp"
							android:background="@drawable/reddit_2"
							android:textSize="12sp"
							android:textColor="#000000"
							android:alpha="0.7"/>
						<LinearLayout
							android:id="@+id/linear40"
							android:layout_width="match_parent"
							android:layout_height="30dp"
							android:padding="8dp"
							android:orientation="horizontal"/>
						<Button
							android:id="@+id/button18"
							android:layout_width="40dp"
							android:layout_height="40dp"
							android:padding="8dp"
							android:background="@drawable/nexina"
							android:textSize="12sp"
							android:textColor="#000000"
							android:alpha="0.7"/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<WebView
				android:id="@+id/webview1"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:background="#FFFFFF"
				android:layout_weight="58"/>
			<LinearLayout
				android:id="@+id/linear3"
				android:layout_width="match_parent"
				android:layout_height="40dp"
				android:background="#FFFFFF"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:weightSum="5"
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_weight="1">
				<LinearLayout
					android:id="@+id/left_box"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:background="#212121"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal"
					android:weightSum="5"
					android:layout_weight="4">
					<LinearLayout
						android:id="@+id/linear14"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<Button
							android:id="@+id/button1"
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:layout_margin="5dp"
							android:padding="8dp"
							android:background="@drawable/ic_home_white"
							android:textSize="12sp"
							android:textColor="#000000"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear7"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<Button
							android:id="@+id/button2"
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:layout_margin="5dp"
							android:padding="8dp"
							android:background="@drawable/left"
							android:textSize="12sp"
							android:textColor="#000000"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear30"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<Button
							android:id="@+id/button12"
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:padding="8dp"
							android:textSize="12sp"
							android:textColor="#000000"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear13"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<Button
							android:id="@+id/button4"
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:layout_margin="5dp"
							android:padding="8dp"
							android:background="@drawable/right"
							android:textSize="12sp"
							android:textColor="#000000"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear12"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:layout_marginRight="10dp"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<Button
							android:id="@+id/button5"
							android:layout_width="25dp"
							android:layout_height="25dp"
							android:layout_margin="5dp"
							android:padding="8dp"
							android:background="@drawable/ic_border_all_white"
							android:textSize="12sp"
							android:textColor="#000000"/>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear5"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:padding="5dp"
					android:background="@android:color/transparent"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical"
					android:layout_weight="1">
					<Button
						android:id="@+id/button10"
						android:layout_width="35dp"
						android:layout_height="35dp"
						android:padding="8dp"
						android:background="@drawable/logo2"
						android:textSize="12sp"
						android:textColor="#000000"/>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
